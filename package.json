{
  "name": "discord-advanced",
  "version": "0.0.5",
  "description": "Optimize discord.js bot development with pre-made utilities and enhancements.",
  "keywords": [
    "discord.js",
    "discord",
    "utils",
    "slekup",
    "nodejs",
    "utility"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/slekup/discord-advanced"
  },
  "license": "ISC",
  "author": "Slekup",
  "exports": {
    ".": {
      "require": "./dist/index.js",
      "import": "./dist/index.mjs",
      "types": "./dist/index.d.ts"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "bin": {},
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "npm run lint:fix && npm run format && npm run build:nocheck",
    "build:nocheck": "rimraf dist && tsup",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "lint": "eslint src/**/* --ext .ts && tsc --noEmit",
    "lint:fix": "eslint --fix src/**/* --ext .ts && tsc --noEmit",
    "precommit": "npm run lint:fix && npm run format",
    "prepare": "husky install"
  },
  "prettier": {
    "arrowParens": "always",
    "bracketSpacing": true,
    "printWidth": 80,
    "quoteProps": "consistent",
    "semi": true,
    "singleQuote": true,
    "tabWidth": 2,
    "trailingComma": "es5",
    "useTabs": false
  },
  "dependencies": {
    "discord.js": "^14.14.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.23.3",
    "@types/node": "^20.9.4",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "eslint": "^8.54.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-jsdoc": "^46.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-tsdoc": "^0.2.17",
    "eslint-prettier": "0.0.0-empty",
    "husky": "^8.0.3",
    "lint-staged": "^15.1.0",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "ts-node": "^10.9.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2"
  }
}
